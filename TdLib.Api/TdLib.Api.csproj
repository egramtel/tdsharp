<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <LangVersion>latest</LangVersion>
        <PackageId>TDLib.Api</PackageId>
        <RootNamespace>TdLib</RootNamespace>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <NoWarn>
            CS1591 <!-- Missing XML comment for publicly visible type or member -->
        </NoWarn>
        <Description>
            .NET bindings for TDLib (Telegram Database Library): https://github.com/tdlib/td

            This is the TL API descrtiptions. For the main binding engine, see TDLib package.
        </Description>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="ChangelogAutomation.MSBuild" Version="2.0.0" PrivateAssets="All" />
        <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    </ItemGroup>
</Project>
